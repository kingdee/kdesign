    "access": "public"
  },
  "tnpm": {
    "mode": "npm"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run tsc && lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "npm run lint:script",
      "prettier .prettierrc --write"
    ],
    "*.test.{ts,tsx,js,jsx}": [
      "npm run test"
    ],
    "*.less": [
      "npm run lint:style"
    ]
  },
